{
  "name": "akademik",
  "version": "1.0.0",
  "description": "Situs akademik menggunakan mongodb, express, vue js dan node js",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js --exec babel-node --presets babel-preset-env",
    "client": "npm run serve --prefix client",
    "dev": "concurrently \"npm run server\"  \"npm run client\""
  },
  "keywords": [
    "node",
    "js",
    "mongodb",
    "express",
    "vue",
    "js"
  ],
  "author": "Idham Kholid",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^2.0.4"
  }
}
